<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 2.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_2_0.dtd">
<tiles-definitions>
    <definition name="base.template" template="/WEB-INF/jsp/template.jsp">
        <put-attribute name="nav" value="" />
        <put-attribute name="header" value="" />
        <put-attribute name="body" value="" />
        <put-attribute name="footer" value="" />
    </definition>
    <definition name="home" extends="base.template">
        <put-attribute name="nav" value="/WEB-INF/jsp/navBar.jsp" />
        <put-attribute name="header" value="/WEB-INF/jsp/defaultHeader.jsp" />
        <put-attribute name="body" value="/WEB-INF/jsp/homeBody.jsp" />
        <put-attribute name="footer" value="/WEB-INF/jsp/defaultFooter.jsp" />
    </definition>
    
    <definition name="defaultbar" extends="base.template">
        <put-attribute name="header" value="/WEB-INF/jsp/navBar.jsp" />
    </definition>
    
    <definition name="defaultfooter" extends="base.template">
        <put-attribute name="footer" value="/WEB-INF/jsp/defaultFooter.jsp" />
    </definition>
    
        
</tiles-definitions>